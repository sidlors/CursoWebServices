<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="AirportManager">
      <implementation-class>com.example.jaxws.server.AirportManager</implementation-class>
    </service>
    <service name="SecureAirportManager">
      <implementation-class>com.example.jaxws.server.SecureAirportManager</implementation-class>
    </service>
    <service name="BetterAirportManager">
      <implementation-class>com.example.jaxws.ejb.BetterAirportManager</implementation-class>
    </service>
    <service name="SingletonAirportManager">
      <implementation-class>com.example.jaxws.ejb.SingletonAirportManager</implementation-class>
    </service>
    <service name="FlightManager">
      <implementation-class>com.example.jaxws.server.FlightManager</implementation-class>
    </service>
    <service name="Planner">
      <implementation-class>com.example.jaxws.ejb.Planner</implementation-class>
    </service>
  </services>
  <clients>
    <client name="airportManager">
      <wsdl-url>http://localhost:8081/airportManager?WSDL</wsdl-url>
      <local-wsdl-file>localhost_8081/airportManager.wsdl</local-wsdl-file>
      <package-name forceReplace="true">com.example.generated</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8081/airportManager?WSDL</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>com.example.generated</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
